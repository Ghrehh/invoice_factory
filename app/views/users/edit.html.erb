<h1>Updating User Info</h1>

<%= form_for @user do |f| %>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.label "Confirm Password" %>
  <br>
  <%= f.password_field :password_confirmation %>
  <br>
  
  
<%= f.submit "Update", class: "line-button" %>
<% end %>